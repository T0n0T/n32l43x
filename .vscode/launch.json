    "version": "0.2.0",
    "configurations": [
        {
            "name": "N32L40x Launch",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "jlink",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/n32l40x.elf",
            "device": "N32L406RB",
            "interface": "swd",
            "svdFile": "${workspaceRoot}/tools/svd/N32L406.svd",
            "runToEntryPoint": "Reset_Handler",
        },
        {
            "name": "N32L40x Attach",
            "type": "cortex-debug",
            "request": "attach",
            "servertype": "jlink",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/n32l40x.elf",
            "device": "N32L406RB",
            "interface": "swd",
            "svdFile": "${workspaceRoot}/tools/svd/N32L406.svd",
            "runToEntryPoint": "Reset_Handler",
        },
        {
            "name": "Remote Debug",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "external",
            "gdbTarget": "localhost:2331",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/n32l40x.elf",
            "svdFile": "${workspaceRoot}/tools/svd/N32L406.svd",
            "runToEntryPoint": "Reset_Handler",
        }
    ]
}